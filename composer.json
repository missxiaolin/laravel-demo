{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "app/ufa": "dev-master",
        "barryvdh/laravel-ide-helper": "^2.4",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "league/flysystem": "^1.0",
        "maatwebsite/excel": "^2.1",
        "overtrue/wechat": "^3.3",
        "predis/predis": "^1.1",
        "prettus/l5-repository": "^2.6",
        "symfony/ldap": "^3.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "core/database"
        ],
        "psr-4": {
            "Huifang\\Web\\": "app-web/app/",
            "Huifang\\Admin\\": "app-admin/app/",
            "Huifang\\": "core/app/"
        }
    },
    "autoload-dev": {
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\"",
            "php -r \"symlink('../.env', 'app-web/.env');\"",
            "php -r \"symlink('../.env', 'app-admin/.env');\""
        ],
        "post-create-project-cmd": [
        ],
        "post-install-cmd": [
            "php app-web/artisan clear-compiled",
            "php app-admin/artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php app-web/artisan clear-compiled",
            "php app-admin/artisan clear-compiled"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        },
        "ufa": {
            "type": "git",
            "url": "git@github.com:missxiaolin/ufa.git"
        }
    }
}
